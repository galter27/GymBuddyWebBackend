{
  "name": "postsapp",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "cp .env.dev .env && nodemon ./src/app.ts",
    "start": "cp .env.dev .env && npx tsc && node ./dist/src/app.js",
    "test": "cp .env.tst .env && npx tsc && jest --detectOpenHandles --forceExit --coverage --runInBand",
    "testAuth": "cp .env.tst .env && npx tsc && jest --detectOpenHandles --forceExit auth.test.ts",
    "testPosts": "cp .env.tst .env && npx tsc && jest --detectOpenHandles --forceExit posts.test.ts",
    "testComments": "cp .env.tst .env && npx tsc && jest --detectOpenHandles --forceExit comments.test.ts",
    "testFile": "cp .env.tst .env && npx tsc && jest --detectOpenHandles --forceExit file.test.ts",
    "lint": "npx eslint . --ext .ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^9.15.1",
    "mongoose": "^8.8.2",
    "multer": "^1.4.5-lts.1",
    "punycode": "^2.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  }
}
